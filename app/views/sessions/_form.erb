<%= form_for :session, url: session_path do |form| %>
  <%= form.label :email, class:'pull-right' %>
  <%= form.text_field :email, type: 'email' %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <button type='submit' class='btn btn-block btn-primary' style='width: 100% !important;'>
    Sign In
  </button>

