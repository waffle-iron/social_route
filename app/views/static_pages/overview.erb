<% provide(:title, "Overview: #{@account_name}") %>
<center>
  <h1><%= "#{@account_name} Overview" %></h1>
</center>


<div class="row">
  <div class="col s4">
    <h4>Campaigns</h4>
    <ul class="collection with-header">
      <% @account_data.each do |campaign| %>
        <li class="collection-item">
          <div>
            <%= campaign['name'] %>
            <a href="#!" class="secondary-content"><i class="material-icons">open_in_new</i></a>
          </div>
        </li>
      <% end %>
    </ul>
  </div>
